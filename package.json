  "build": {
    "appId": "com.retail-reaction",
    "directories": {
      "buildResources": "desktop/resources",
      "output": "packages",
      "app": "dist"
    },
    "mac": {
      "category": "public.app-category.retail",
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "snap"
      ]
    },
    "asar": true
